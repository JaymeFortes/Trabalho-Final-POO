<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="aplicacao.TransportesPendentes">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
        <TextArea fx:id="txtMensagem" layoutY="1.0" prefHeight="375.0" prefWidth="640.0" />
        <Button fx:id="buttonVoltar" onAction="#voltarParaMenuPrincipal" layoutY="337.0" mnemonicParsing="false" text="Voltar" />
        <Button fx:id="buttonSair" layoutX="580.0" layoutY="337.0" mnemonicParsing="false" text="Sair" />
        <Button fx:id="buttonProcessarPendentes" onAction="#processarTransportesPendentes" layoutX="400.0" layoutY="337.0" mnemonicParsing="false" text="Processar Transportes Pendentes" />
      </children>
    </AnchorPane>
  </children>
</VBox>
